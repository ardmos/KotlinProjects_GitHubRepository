<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/wood_main"
        android:layout_width="321dp"
        android:layout_height="350dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.346"
        app:srcCompat="@drawable/wood" />

    <ImageView
        android:id="@+id/orange_main"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/wood_main"
        app:layout_constraintEnd_toEndOf="@+id/wood_main"
        app:layout_constraintStart_toStartOf="@+id/wood_main"
        app:layout_constraintTop_toTopOf="@+id/wood_main"
        app:layout_constraintVertical_bias="0.44"
        app:srcCompat="@drawable/orange__1_" />

    <ImageView
        android:id="@+id/wood_left"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/wood_plank"
        app:layout_constraintBottom_toBottomOf="@+id/wood_main"
        app:layout_constraintEnd_toStartOf="@+id/wood_main"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/wood_main" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/wood_left"
        app:layout_constraintStart_toEndOf="@+id/wood_left"
        app:layout_constraintStart_toStartOf="@+id/wood_left"
        app:layout_constraintTop_toTopOf="@+id/wood_left"
        app:srcCompat="@drawable/down_arrow_h" />

    <ImageView
        android:id="@+id/orange_left"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/wood_left"
        app:layout_constraintEnd_toEndOf="@+id/wood_left"
        app:layout_constraintStart_toStartOf="@+id/wood_left"
        app:layout_constraintTop_toTopOf="@+id/wood_left"
        app:layout_constraintVertical_bias="0.44"
        app:srcCompat="@drawable/orange__1_" />

    <ImageView
        android:id="@+id/wood_bottom"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/wood_plank_h"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wood_main" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toTopOf="@+id/wood_bottom"
        app:layout_constraintEnd_toEndOf="@+id/wood_bottom"
        app:layout_constraintStart_toStartOf="@+id/wood_bottom"
        app:srcCompat="@drawable/down_arrow" />

    <ImageView
        android:id="@+id/orange_bottom"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/wood_bottom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/wood_bottom"
        app:layout_constraintTop_toTopOf="@+id/wood_bottom"
        app:srcCompat="@drawable/orange__1_" />

    <com.google.android.gms.ads.AdView
        xmlns:ads="http://schemas.android.com/apk/res-auto"
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="ca-app-pub-3940256099942544/6300978111">
    </com.google.android.gms.ads.AdView>

</androidx.constraintlayout.widget.ConstraintLayout>